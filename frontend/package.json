  "scripts": {
    "build": "ember build --environment=production",
    "lint": "eslint . --max-warnings 0 && ember-template-lint . --max-warnings 0 && stylelint app/styles/**/*.scss --max-warnings 0",
    "lint:ts": "eslint . --max-warnings 0",
    "lint:ts:fix": "eslint . --fix",
    "lint:hbs": "ember-template-lint . --max-warnings 0",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "format:hbs": "prettier **/*.hbs --write --parser=glimmer",
    "lint:scss": "stylelint app/styles/**/*.scss --max-warnings 0",
    "lint:scss:fix": "stylelint app/styles/**/*.scss --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "lint-staged": {
    "linters": {
      "*.js": "eslint . --max-warnings=0",
      "*.ts": "eslint . --max-warnings=0",
      "*.scss": "stylelint app/styles/**/*.scss --max-warnings=0",
      "*.hbs": "ember-template-lint . --max-warnings=0"
    },
    "ignore": [
      "vendor/*"
    ]
  },
